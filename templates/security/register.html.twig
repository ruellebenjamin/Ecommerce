{% extends "base.html.twig" %}

{% block body %}
    {{ form_start(form)}}
    <div>  
        <div>
            {{
                form_row(form.email)
            }}
        </div>
    </div>
    <div>
        <div>
            {{ form_row(form.password)}}
        </div>
    </div>
    <button type="submit">send</button>
    <p>Déjà inscrit ? <a class="underline" href="/login">Connectez-vous !</a></p>
    {{ form_end(form) }}
{% endblock %}